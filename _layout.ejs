      <%- partial('partials/_hero', { title: 'Front End Development' }) %>
    <% } %>
  </header>
  <main class="container">
    <%- yield %>
  </main>
  <footer class="footer container">
    <p>&copy; 2018</p>
  </footer>
</body>
</html>